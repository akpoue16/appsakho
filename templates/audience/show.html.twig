{% extends 'base.html.twig' %}

{% block title %}Audience{% endblock %}

{% block body %}
    <h1>Audience</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ audience.id }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ audience.code }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ audience.createdAt ? audience.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Conseil</th>
                <td>{{ audience.conseil }}</td>
            </tr>
            <tr>
                <th>Motif</th>
                <td>{{ audience.motif }}</td>
            </tr>
            <tr>
                <th>Procedures</th>
                <td>{{ audience.procedures }}</td>
            </tr>
            <tr>
                <th>Renvoyer</th>
                <td>{{ audience.renvoyer ? audience.renvoyer|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>NomPresident</th>
                <td>{{ audience.nomPresident }}</td>
            </tr>
            <tr>
                <th>NomGreffier</th>
                <td>{{ audience.nomGreffier }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_audience_index') }}">back to list</a>

    <a href="{{ path('app_audience_edit', {'id': audience.id}) }}">edit</a>

    {{ include('audience/_delete_form.html.twig') }}
{% endblock %}
