<page style="font-size: 14px" backtop="20mm" backbottom="20mm" backleft="10mm" backright="10mm">
	<page_header>
		<table style="width: 100%; border: solid 1px black;">
			<tr>
				<td style="text-align: left;    width: 33%">Cabinet Sakho</td>
				<td style="text-align: center;    width: 34%"></td>
				<td style="text-align: right;    width: 33%">{{ "now"|date("d/m/Y") }}</td>
			</tr>
		</table>
	</page_header>
	<page_footer>
		<table style="width: 100%; border: solid 1px black;">
			<tr>
				<td style="text-align: left; width: 50%">Cabinet Sakho</td>
				<td style="text-align: right; width: 50%">page [[page_cu]]/[[page_nb]]</td>
			</tr>
		</table>
	</page_footer>
	<div class="container">
		<h5 style="text-align:right" class="fs-6">Audience du
			{{ search|format_datetime(locale='fr',pattern="EEEE dd MMMM YYYY")  }}
		</h5>
	</div>

	{% for audience in audiences %}
		<table class="table1" style="width: 100%;margin:0px;padding:0px;">
			<tr>
				<td>
					Juridiction :
					{{ audience.juridiction.titre}}

				</td>
				<td style=" text-align:center">
					{{ audience.createdAt|date('H') }}
					H
					{{ audience.createdAt|date('i') }}
				</td>
				<td>
					<ul>
						<li>AVOCAT:
							{{ audience.avocat.nom }}
							{{ audience.avocat.prenom }}</li>
						<li>PRESIDENT:
							{{ audience.nomPresident }}</li>
						<li>GREFFIER:
							{{ audience.nomGreffier }}</li>
					</ul>
				</td>
			</tr>
		</table>
		<table class="table1 tableAudience" style="width: 100%; margin:0px;padding:0px;">
			<tr style=" border-bottom:1px sold black">
				<td style="width:10%; text-align:center">
					<h6>...
					</h6>
					<p style="margin:0px;padding:0px">Date Prc.</p>
					<h5 style="margin:0px;padding:0px">
						{% if audience.audiencePre %}
							{{ audience.audiencePre.createdAt|date('d/m/Y')  }}
						{% else %}
							A.N.
						{% endif %}
					</h5>
				</td>
				<td style="width:10%;text-align:center">
					<p style="margin:0px;padding:0px">Dossier</p>
					<h5 style="margin:0px;padding:0px">{{ audience.contentieux.code }}</h5>
					<p style="margin:0px;padding:0px">({{ audience.contentieux.createdAt|date('Y') }})</p>
					<p>{{ audience.contentieux.rg }}</p>
				</td>
				<td style="width:10%;font-size:9px;">
					<p style="transform: rotate(45deg)">{{ audience.contentieux.qualite.titre }}</p>
					<p style="transform: rotate(45deg)">{{ audience.contentieux.qualite.titre }}</p>
				</td>
				<td style="width:30%">
					<p>Client:
						{{ audience.contentieux.client.nom }}
						{{ audience.contentieux.client.prenom }}</p>
					<p>Adv:
						{{ audience.contentieux.adversaire.nom }}
						{{ audience.contentieux.adversaire.prenom }}</p>
					<p>
						Conseil:
						{{ audience.conseil }}
					</p>
				</td>
				<td>
					<p style="width:30%;text-align:center">Procédure</p>
					<h6 style="margin-bottom:0px;">{{ audience.procedures }}</h6>
					<p>Motif Précédent</p>

					{% if audience.audiencePre %}
					<h6 style="margin-top:0px;">{{ audience.audiencePre.resultatAudience|raw }}</h6>
					{% else %}
					<h6> </h6>
					{% endif %}
				</td>
				<td style="width:10%">
					<p >Renvoyer au</p>
					{% if audience.resultatAudience %}
					<h6 style="margin-top:0px">{{ audience.resultatAudience.createdAt|date('d/m/Y') }}</h6>
					<p>{{ audience.resultatAudience.resultat|raw }}</p>
					{% else %}
					<h6> </h6>
					{% endif %}
				</td>
			</tr>
		</table>
	{% endfor %}
</page>
