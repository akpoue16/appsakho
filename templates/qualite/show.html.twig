{% extends 'base.html.twig' %}

{% block title %}qualite
{% endblock %}

{% block body %}
	<!-- Page Heading -->
	<div class="d-sm-flex align-items-center justify-content-between mb-4">
		<h1 class="h3 mb-0 text-gray-800">Qualite:
			{{ qualite.titre }}
			
	</div>
	<!-- DataTales Example -->
	<div
		class="card shadow mb-4">
		<div class="card-body">

			<div class="row">
				<div class="col-md-8">
					<table class="table">
						<tbody>
							
							<tr>
								<th>Titre</th>
								<td>{{ qualite.titre }}</td>
							</tr>
							
						</tbody>
					</table>
				</div>
				<div class="col-md-4">
				
				</div>
			</div>
			<div class="row">
				<div class="col">
					<a class="btn btn-primary btn-icon-split" href="{{ path('app_qualite_index') }}">
						<span class="icon text-white-10">
							<i class="fas fa-arrow-left fa-sm text-white-10"></i>
						</span>
						<span class="text">Retour Ã  la liste</span>
					</a>
					<a class="btn btn-success btn-icon-split" href="{{ path('app_qualite_edit', {'id': qualite.id}) }}">
						<span class="icon text-white-10">
							<i class="fas fa-pencil-alt fa-sm text-white-10"></i>
						</span>
						<span class="text">
							Modifier</span>
					</a>
				</div>
			</div>

		</div>
	</div>
{% endblock %}
